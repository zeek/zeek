macro EvalStartsWith(str_val, sub_val)
	auto str = str_val.string_val;
	auto sub = sub_val.string_val;
	auto str_n = str->Len();
	auto sub_n = sub->Len();
	if ( str_n < sub_n )
		frame[z.v1].int_val = 0;
	else
		{
		auto str_b = str->Bytes();
		auto sub_b = sub->Bytes();
		int i;
		for ( i = 0; i < sub_n; ++i )
			if ( str_b[i] != sub_b[i] )
				break;
		frame[z.v1].int_val = i == sub_n;
		}

