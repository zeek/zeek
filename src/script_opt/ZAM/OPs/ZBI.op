macro TelemetryCounterInc(result, family, increment)
	{
	if ( auto ptr = static_cast<CounterMetricVal*>(family) )
		{
		ptr->GetHandle()->Inc(increment);
		result = 1;
		}
	else
		{
		zeek::reporter->Error("Telemetry::counter_inc: invalid handle.");
		result = 0;
		}
	}

