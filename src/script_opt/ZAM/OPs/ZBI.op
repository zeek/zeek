internal-op Lookup-Conn
class VV
op-types X R
eval	auto cid = $1;
	Connection* conn = ZAM::session_mgr_find_connection(cid);
	ValPtr res;
	if ( conn )
		res = conn->GetVal();
	else
		{
		emit_builtin_error("connection ID not a known connection", cid);
		res = build_dummy_conn_record();
		}
	AssignTarget($$, ZVal(res, res->GetType()));

