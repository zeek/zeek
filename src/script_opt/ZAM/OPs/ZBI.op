internal-op Starts-With
classes VVV VCV VVC
op-types I S S
eval	auto str = $1;
	auto sub = $2;
	auto str_n = str->Len();
	auto sub_n = sub->Len();
	if ( str_n < sub_n )
		$$ = 0;
	else
		{
		auto str_b = str->Bytes();
		auto sub_b = sub->Bytes();
		int i;
		for ( i = 0; i < sub_n; ++i )
			if ( str_b[i] != sub_b[i] )
				break;
		$$ = i == sub_n;
		}

